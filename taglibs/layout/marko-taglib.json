{
    "<layout-use>": {
        "@template": "template",
        "@*": {
            "remove-dashes": true,
            "type": "string"
        },
        "renderer": "./use-tag",
        "body-function": "getContent(__layoutHelper)"
    },
    "<layout-put>": {
        "@into": "string",
        "@value": "string",
        "renderer": "./put-tag",
        "import-var": {
            "layout": "__layoutHelper"
        }
    },
    "<layout-placeholder>": {
        "@name": "string",
        "renderer": "./placeholder-tag",
        "import-var": {
            "content": "data.layoutContent"
        }
    }
}
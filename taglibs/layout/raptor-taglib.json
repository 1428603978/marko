{
    "tags": {
        "layout-use": {
            "renderer": "./UseTag",
            "attributes": {
                "template": {
                    "type": "path"
                },
                "*": {
                    "remove-dashes": true,
                    "type": "string"
                }
            },
            "var": "_layout"
        },
        "layout-put": {
            "renderer": "./PutTag",
            "attributes": {
                "into": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                }
            },
            "import-var": {
                "_layout": "_layout"
            }
        },
        "layout-placeholder": {
            "renderer": "./PlaceholderTag",
            "attributes": {
                "name": {
                    "type": "string"
                }
            },
            "import-var": {
                "content": "data.layoutContent"
            }
        }
    }
}